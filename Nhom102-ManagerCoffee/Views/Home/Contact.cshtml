@{
    ViewBag.Title = "Contact";
}
<div class="inner-banner">
</div>
<section class="w3l-breadcrumb">
    <div class="container">
        <ul class="breadcrumbs-custom-path">
            <li><a href="#url">Trang Chủ</a></li>
            <li class="active"><span class="fa fa-arrow-right mx-2" aria-hidden="true"></span>Liên Hệ Với Chúng Tôi</li>
        </ul>
    </div>
</section>
<!-- /contact-form -->
<section class="w3l-contact-6 py-5" id="contact">
    <div class="contact-info  py-lg-4 py-md-3">
        <div class="container">
            <div class="title-content mb-5">
                <h5 class="title-small text-center mb-2">Liên Hệ Với Chúng Tôi</h5>
            </div>
            <div class="grid contact-grids pt-3">
                <div class="contact-left">
                    <div class="grid">
                        <span class="fa fa-map-marker"></span>
                        <div class="location-info">
                            <span>Địa Chỉ</span>
                            <p> Số 280</p>
                            <p>Đường: An Dương Vương,Phường 4</p>
                            <p>Quận 5, TPHCM</p>
                        </div>
                    </div>
                    <div class="grid">
                        <span class="fa fa-envelope-o"></span>
                        <div class="email-info">
                            <span>Mail</span>
                            <a href="mailto:mail@examplemail.com"> TienLe10998@gmail.com</a>
                        </div>
                    </div>
                    <div class="grid">
                        <span class="fa fa-phone"></span>
                        <div class="email-info">
                            <span>Số Điện Thoại</span>
                            <a href="tel:(84) 334-303-506"> +(84)-334-303-506</a><br>
                        </div>
                    </div>
                </div>
                <div class="contacts12-main">
                    <form action="https://sendmail.w3layouts.com/submitForm" method="post" class="signin-form">
                        <div class="input-grids">
                            <div>
                                <label class="form-field" for="w3lName">Tên</label>
                                <input type="text" name="w3lName" id="w3lName" placeholder="Nguyễn Văn A"
                                       class="contact-input" />
                            </div>
                            <div>
                                <label class="form-field" for="w3lSender">Email</label>
                                <input type="email" name="w3lSender" id="w3lSender" placeholder="abc@gmail.com"
                                       class="contact-input" />
                            </div>
                        </div>

                        <div>
                            <label class="form-field" for="w3lSubect">Vấn Đề</label>
                            <input type="text" name="w3lSubect" id="w3lSubect" placeholder="Vấn Đề"
                                   class="contact-input" />
                        </div>
                        <div>
                            <label class="form-field" for="w3lMessage">Nội Dung</label>
                            <textarea name="w3lMessage" id="w3lMessage" placeholder="Hãy Nhập Nội Dung"
                                      required=""></textarea>
                        </div>
                        <div class="text-right">
                            <button class="btn btn-style btn-primary submit">Gửi</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
</section>
<!-- //contact-form -->
