<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Manager</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    @RenderBody()



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="~/Areas/Admin/assets/js/jquery-3.3.1.js"></script>

    <script src="~/Areas/Admin/assets/js/bootstrap.js"></script>
    <script src="~/Areas/Admin/assets/js/xuLy.js"></script>
    <script>
        $(document).ready(function () {
            $('#table-1').click(function () {
                $('body').css({ 'padding-right': '0px' });
            })

        });

    </script>
</body>
</html>